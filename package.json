{
  "name": "tunnel-quest-bot",
  "version": "1.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npx prisma generate && npx prisma migrate deploy && node ./build/index.js",
    "debug": "npx prisma generate && npx prisma migrate deploy && node --inspect=0.0.0.0 ./build/index.js",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"./**/*.ts\"",
    "dev": "concurrently \"npm run fakeLogs\" \"npm run debug\"",
    "fakeLogs": "node ./build/lib/parser/logFaker.js",
    "clean": "rd /s /q .\\build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write \"./**/*.ts\"",
      "eslint --fix ."
    ]
  },
  "keywords": [],
  "author": "jamesjamail",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^2.0.1",
    "@prisma/client": "^6.19.2",
    "@types/node": "^25.0.10",
    "@types/tail": "^2.2.1",
    "ahocorasick": "^1.0.2",
    "axios": "^1.5.1",
    "chalk": "^5.6.2",
    "concurrently": "^9.2.1",
    "date-fns": "^4.1.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.1.0",
    "ioredis": "^5.3.2",
    "node-forge": "^1.3.1",
    "tail": "^2.2.6"
  },
  "devDependencies": {
    "@types/node-forge": "^1.3.7",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "eslint-config-love": "^147.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.0.3",
    "prisma": "^6.19.2",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  }
}
