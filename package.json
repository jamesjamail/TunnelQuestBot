{
  "name": "tunnel-quest-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "rd /s /q .\\build && tsc && cls && node .\\build\\index.js",
    "build": "tsc",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"./**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write \"./**/*.ts\"",
      "eslint --fix ."
    ]
  },
  "keywords": [],
  "author": "jamesjamail",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "@prisma/client": "^5.1.1",
    "@types/node": "^18.7.14",
    "chalk": "^4.1.2",
    "date-fns": "^2.30.0",
    "discord.js": "14.13.0",
    "dotenv": "^16.0.2",
    "fuse.js": "^6.6.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.1.1",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "discord-api-types": "0.37.20"
  },
  "prisma": {
    "schema": ".\\src\\prisma\\schema.prisma"
  }
